# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Schedule {
  id: ID!
  text: String!
  user: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  schedules: [Schedule!]!
}

input NewSchedule {
  text: String!
  userId: String!
}

type Mutation {
  createSchedule(input: NewSchedule!): Schedule!
}
